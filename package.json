{
  "name": "test-tube",
  "version": "0.0.0",
  "description": "Test React components from the point of view of a user",
  "keywords": [
    "react",
    "test"
  ],
  "homepage": "https://github.com/vinsonchuong/test-tube",
  "bugs": "https://github.com/vinsonchuong/test-tube/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/test-tube",
  "type": "module",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "dependencies": {
    "js-beautify": "^1.14.4",
    "jsdom": "^20.0.1",
    "jsdom-global": "^3.0.2",
    "ms": "^2.1.3"
  },
  "peerDependencies": {
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0"
  },
  "devDependencies": {
    "ava": "^4.3.0",
    "dynamic-script": "^1.0.3",
    "get-port": "^6.1.2",
    "htm": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "semantic-release": "^19.0.2",
    "testdouble": "^3.16.6",
    "xo": "^0.50.0"
  },
  "ava": {
    "verbose": "true"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "import/no-anonymous-default-export": "off",
      "import/no-unassigned-import": "off",
      "n/file-extension-in-import": "off",
      "node/file-extension-in-import": "off"
    },
    "globals": [
      "window",
      "document"
    ]
  }
}
