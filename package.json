{
  "name": "test-tube",
  "version": "0.0.0",
  "description": "Test React components from the point of view of a user",
  "keywords": [
    "react",
    "test"
  ],
  "homepage": "https://github.com/vinsonchuong/test-tube",
  "bugs": "https://github.com/vinsonchuong/test-tube/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/test-tube",
  "scripts": {
    "prepare": "flow-typed update --skip",
    "test": "standard -v --fix && flow && ava -v",
    "release": "semantic-release",
    "prepack": "build-esm"
  },
  "dependencies": {
    "jsdom": "^14.0.0",
    "jsdom-global": "^3.0.2"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "build-esm": "^4.2.1",
    "flow-bin": "^0.95.1",
    "flow-typed": "^2.5.1",
    "overdub": "^2.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "semantic-release": "^15.13.3",
    "standard-esnext": "^5.3.0",
    "testdouble": "^3.11.0"
  },
  "ava": {
    "require": "overdub/register",
    "babel": {
      "testOptions": {
        "extends": "overdub/babel"
      }
    }
  }
}
