{
  "name": "test-tube",
  "version": "0.0.0",
  "description": "Test React components from the point of view of a user",
  "keywords": [
    "react",
    "test"
  ],
  "homepage": "https://github.com/vinsonchuong/test-tube",
  "bugs": "https://github.com/vinsonchuong/test-tube/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/test-tube",
  "scripts": {
    "prepare": "flow-typed update --skip",
    "test": "standard -v --fix && flow && ava -v",
    "release": "semantic-release",
    "prepack": "build-esm"
  },
  "dependencies": {
    "jsdom": "^15.2.0",
    "jsdom-global": "^3.0.2",
    "ms": "^2.1.2",
    "pretty": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "build-esm": "^4.2.2",
    "dynamic-script": "^1.0.3",
    "flow-bin": "^0.110.0",
    "flow-typed": "^2.6.1",
    "get-port": "^5.0.0",
    "overdub": "^2.5.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "semantic-release": "^15.13.27",
    "standard-esnext": "^5.3.3",
    "testdouble": "^3.12.4"
  },
  "ava": {
    "require": "overdub/register",
    "babel": {
      "testOptions": {
        "extends": "overdub/babel"
      }
    }
  }
}
